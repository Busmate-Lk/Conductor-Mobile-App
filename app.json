{
  "expo": {
    "name": "BusmateLKConductorApp",
    "slug": "BusmateLKConductorApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/busmate_logo.png",
    "scheme": "busmatelkconductorapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/busmate_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.manushakawshan.BusmateLKConductorApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/busmate_logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BusMate to access your camera to scan QR codes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ea7d5120-48a9-4472-a582-5bee0df723fe"
      }
    }
  }
}
